{
    "name": "yl-11ty",
    "version": "0.9.3",
    "description": "An 11ty starter kit",
    "scripts": {
        "start": "npm run dev",
        "build": "run-s clean eleventy:prod --print-label",
        "dev": "run-s clean eleventy:dev --print-label",
        "clean": "del-cli _site",
        "eleventy:dev": "ELEVENTY_ENV=development eleventy --serve",
        "eleventy:prod": "ELEVENTY_ENV=production eleventy",
        "lint": "run-s lint:js lint:scss --print-label",
        "lint:js": "eslint src/js/**/*",
        "lint:scss": "stylelint --syntax scss src/css/**/*.scss",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "core-js": "^3.6.5",
        "luxon": "^1.25.0"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "@11ty/eleventy-plugin-rss": "^1.0.9",
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "autoprefixer": "^9.8.6",
        "babel-loader": "^8.1.0",
        "clean-css": "^4.2.3",
        "cssesc": "^3.0.0",
        "del-cli": "^3.0.1",
        "eslint": "^7.8.1",
        "html-minifier": "^4.0.0",
        "memfs": "^3.2.0",
        "node-sass": "^4.14.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^7.0.32",
        "postcss-flexbugs-fixes": "^4.2.1",
        "stylelint": "^13.7.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-scss": "^3.18.0",
        "webpack": "^4.44.1"
    },
    "author": "Matthias Mees",
    "license": "MIT",
    "engines": {
        "node": ">=14.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:yellowled/yl-11ty.git"
    }
}
