{
    "name": "yl-11ty",
    "version": "0.9.0",
    "description": "My eleventy starter kit",
    "scripts": {
        "start": "npm run dev",
        "build": "run-s clean eleventy:prod --print-label",
        "dev": "run-s clean eleventy:dev --print-label",
        "clean": "del-cli dist",
        "eleventy:dev": "ELEVENTY_ENV=development eleventy --serve",
        "eleventy:prod": "ELEVENTY_ENV=production eleventy",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "luxon": "^1.23.0"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.10.0",
        "@11ty/eleventy-plugin-rss": "^1.0.7",
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "babel-loader": "^8.1.0",
        "clean-css": "^4.2.3",
        "cssesc": "^3.0.0",
        "del-cli": "^3.0.0",
        "html-minifier": "^4.0.0",
        "memfs": "^3.1.2",
        "node-sass": "^4.14.0",
        "npm-run-all": "^4.1.5",
        "webpack": "^4.43.0"
    },
    "author": "Matthias Mees",
    "license": "MIT",
    "engines": {
        "node": ">=10.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:yellowled/yl-11ty.git"
    }
}
